<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cube_background"
    tools:context=".ResultPage">

    <TextView
        android:id="@+id/textView_result"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.797" />

    <TextView
        android:id="@+id/textView_2nd"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207" />

    <TextView
        android:id="@+id/textView_1st"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128" />

    <TextView
        android:id="@+id/textView_4th"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.361" />

    <Button
        android:id="@+id/btn_save"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:fontFamily="@font/carter_one"
        android:text="Save results"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.562" />

    <TextView
        android:id="@+id/textView_3rd"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.284" />

    <TextView
        android:id="@+id/textView_average"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:text="This round average"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.707" />

    <TextView
        android:id="@+id/textView_5th"
        android:layout_width="150dp"
        android:layout_height="31dp"
        android:background="@drawable/text_views"
        android:fontFamily="@font/carter_one"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.44" />

    <Button
        android:id="@+id/btn_tryAgain"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:fontFamily="@font/carter_one"
        android:text="Try again"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.562" />

</androidx.constraintlayout.widget.ConstraintLayout>